    "type": "git",
    "url": "git+https://github.com/lamartire/kirpichik.git"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "^.+\\.spec\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  },
  "keywords": [
    "scaffolding",
    "components",
    "kirpichik"
  ],
  "author": "lamartire",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/lamartire/kirpichik/issues"
  },
  "homepage": "https://github.com/lamartire/kirpichik#readme",
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/handlebars": "^4.0.36",
    "@types/jest": "^21.1.4",
    "@types/minimist": "^1.2.0",
    "@types/node": "^8.0.46",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "ts-jest": "^21.1.3",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^6.0.1",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "axios": "^0.17.0",
