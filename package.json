    "type": "git",
    "url": "git+https://github.com/lamartire/kirpichik.git"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "^.+\\.spec\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  },
  "keywords": [
    "scaffolding",
    "components",
    "kirpichik"
  ],
  "author": "lamartire",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/lamartire/kirpichik/issues"
  },
  "homepage": "https://github.com/lamartire/kirpichik#readme",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/handlebars": "^4.0.36",
    "@types/jest": "^21.1.8",
    "@types/minimist": "^1.2.0",
    "@types/node": "^8.5.1",
    "jest": "^22.0.3",
    "jest-cli": "^22.0.3",
    "prettier": "^1.9.2",
    "ts-jest": "^22.0.0",
    "tslint": "^5.8.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "chalk": "^2.3.0",
    "change-case": "^3.0.1",
    "handlebars": "^4.0.11",
    "minimist": "^1.2.0"
  }
